{
  "name": "rdf-document-editor",
  "version": "0.1.0",
  "homepage": "https://github.com/buda-base/rdf-document-editor/",
  "description": "RDF Document Editor",
  "author": "Nicolas Berger and Elie Roux",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/leaflet": "^1.8.0",
    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-hash-link": "^2.4.0",
    "@uiw/react-md-editor": "^3.0.9",
    "debug": "^4.2.0",
    "i18next": "^19.8.9",
    "leaflet": "^1.7.1",
    "leaflet-geosearch": "^3.6.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.16",
    "node-sass-chokidar": "^1.5.0",
    "query-string": "^7.0.0",
    "rdflib": "^2.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-i18next": "^11.8.7",
    "react-image": "^4.0.3",
    "react-leaflet": "^3.2.5",
    "react-leaflet-google-layer": "^2.0.5",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "react-router-hash-link": "^2.4.3",
    "recoil": "^0.7.1",
    "typescript": "^4.2.2",
    "typescript-memoize": "^1.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/stylesheets -o public/ --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar src/stylesheets -o public/ --watch --recursive",
    "analyze": "source-map-explorer 'dist/static/js/*.js'",
    "predeploy": "npm run build",
    "lint": "eslint --ignore-path .gitignore src/ && dpdm --exit-code circular:1 src/index.ts",
    "rollup": "./node_modules/rollup/dist/bin/rollup -c rollup.config.js && yarn fix-materialui",
    "build": "npm run build-css && yarn rollup",
    "fix-materialui": "sed -i -e \"s/\\(material-ui\\/core.*\\)\\(['\\\"]\\).*$/\\1\\/index.js\\2/g\" -e \"s/\\(material-ui\\/icons.*\\)\\(['\\\"]\\).*$/\\1.js\\2/g\" dist/index.mjs"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,tsx,ts}": [
      "prettier --write --config ./.prettierrc",
      "eslint --color --fix"
    ],
    "demo/**/*.{js,jsx,json,css,tsx,ts}": [
      "prettier --write --config ./.prettierrc",
      "eslint --color --fix"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "dpdm": "^3.10.0",
    "esbuild": "^0.15.11",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "preval.macro": "^5.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.3",
    "rollup-plugin-esbuild": "^4.10.1",
    "type-fest": "^0.13.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts"
}
